{
    "name": "drupal/helfi_api_base",
    "type": "drupal-module",
    "description": "Helfi - API Base",
    "license": "GPL-2.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "drupal/big_pipe_sessionless": "^2.0",
        "drupal/entity": "^1.0",
        "drupal/health_check": "^3.0",
        "drupal/monolog": "^3.0",
        "drupal/raven": "^5.0 || ^6.0",
        "firebase/php-jwt": "^7.0",
        "php": "^8.1",
        "t4web/composer-lock-parser": "^1.0",
        "textalk/websocket": "^3.6",
        "webmozart/assert": "^1.0",
        "ext-curl": "*",
        "symfony/polyfill-php84": "^1.0",
        "symfony/polyfill-php85": "^1.0",
        "city-of-helsinki/php-logger-extra": "^1.0"
    },
    "repositories": {
        "php-logger-extra": {
            "type": "vcs",
            "url": "https://github.com/City-of-Helsinki/php-logger-extra.git"
        }
    },
    "conflict": {
        "drupal/helfi_debug": "*"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.0",
        "drupal/coder": "^8.3",
        "drupal/search_api": "^1.0",
        "drupal/elasticsearch_connector": "^8.0@alpha",
        "donatj/mock-webserver": "dev-master"
    },
    "extra": {
        "patches": {
                "drupal/big_pipe_sessionless": {
                        "Drupal 11 support (https://drupal.org/i/3428235)": "https://www.drupal.org/files/issues/2025-05-07/3428235.d11.patch"
                }
        }
    }
}
